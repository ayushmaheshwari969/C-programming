#include <stdio.h>

int main() {
   int n, prime = 1;
   
   printf("Enter a value");
   scanf("%d", &n);
   for(int i=2; i<=n-1 ; i++)
   {
      if (n%i ==0)
      prime  =0 ;
   }
   if (prime == 0)
   printf(" %d is a prime number", n);
   
   else
   printf("%d is s prime number",n);
    
    return 0;
}
