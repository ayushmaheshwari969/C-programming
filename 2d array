#include <stdio.h>

int main()
{
     int marks [2][2];
     
    for(int i = 0 ; i <2; i++)
    {
         printf("i loop\n");
        for(int j = 0 ; j < 2 ; j++)
        {
            printf("print coordinates of %d , %d\n" , i , j );
           scanf("%d" , &marks[i][j]);
        }
      
    }
    
    
    for(int x = 0 ; x < 2 ; x++)
    {
       for(int y = 0 ; y < 2 ; y++)
       {
           printf("%d , ", marks[x][y]);
       }
       printf("\n");
    }
    
    return 0;
}
